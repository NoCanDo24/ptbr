      <box size="0.1634 0.08 0.06"/>
    </geometry>
    <material name="silver"/>
  </collision>
  <collision>
    <origin xyz="-0.0327525 0.0 0.0575" rpy="0.0 0.0 0.0"/>
    <geometry>
      <box size="0.032895 0.032895 0.055"/>
    </geometry>
    <material name="silver"/>
  </collision>
</link>

<joint name="head_joint" type="revolute">
  <origin xyz="-0.0327525 0 0.085" rpy="0 0 0"/>
