    <inertia ixx="0.029478" iyy="0.029097" izz="0.015168" ixy="0.0" iyz="0.000325" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0.138" rpy="0 0 0"/>
    <geometry>
      <box size="0.1 0.1 0.1"/>
    </geometry>
    <material name="silver"/>
  </visual>

  <visual>
    <origin xyz="0.041 0 0.138" rpy="0.0 0.0 0.0"/>
    <geometry>
      <box size="0.018 0.106 0.106"/>
    </geometry>
  </visual>
  <visual>
    <origin xyz="0 0 0.024" rpy="0.0 0.0 0.0"/>
    <geometry>
      <box size="0.038895 0.038895 0.048"/>
