    <geometry>
      <box size="0.0525 0.0525 0.04"/>
    </geometry>
  </collision>
</link>




<joint name="right_wheel_joint" type="continuous">
  <origin xyz="0 -0.06 0" rpy="${pi/2} 0 0"/>
  <parent link="base_link"/>
  <child link="right_wheel"/>
  <axis xyz="0 0.0 -1"/>
  <limit effort="50000" velocity="10"/>
  <dynamics damping="1.0" friction="1.0"/>
</joint>

<link name="right_wheel">
  <inertial>
    <origin xyz="-0.019999714915771002 -2.0220914574015092e-07 2.2264549151551627e-07" rpy="0 0 0"/>
    <mass value="1"/>
    <inertia ixx="0.000659" iyy="0.000482" izz="0.000482" ixy="0.0" iyz="0.0" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <cylinder radius="0.0325" length="0.04"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <cylinder radius="0.0325" length="0.04"/>
