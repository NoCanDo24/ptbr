<?xml version="1.0" ?>
<robot name="ptbr" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find ptbr)/urdf/materials.xacro" />
<xacro:include filename="$(find ptbr)/urdf/ptbr.ros2control" />
<xacro:include filename="$(find ptbr)/urdf/ptbr.gazebo" />

<xacro:include filename="$(find ptbr)/urdf/camera.xacro" />

<link name="base_footprint">
  
</link>

<joint name="base_link_joint" type="fixed">
  <origin xyz="0 0 0.0325"/>
  <parent link="base_footprint"/>
  <child link="base_link"/>
  
</joint>

<link name="base_link">
</link>

<joint name="base_joint" type="fixed">
  <parent link="base_link"/>
  <child link="base"/>
</joint>

<link name="base">
  <inertial>
    <origin xyz="0.006425674318858227 -0.00030771491577107806 0.038113735190444614" rpy="0 0 0"/>
    <mass value="10"/>
    <inertia ixx="0.018437" iyy="0.007171" izz="0.017545" ixy="0.0" iyz="-0.001397" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0.006" rpy="0 0 0"/>
    <geometry>
      <box size="0.1634 0.08 0.06"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <visual>
    <origin xyz="-0.0327525 0.0 0.0575" rpy="0.0 0.0 0.0"/>
    <geometry>
      <box size="0.032895 0.032895 0.055"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0 0 0.006" rpy="0 0 0"/>
